rbbt=/$PWD filter="*.rb *.rake *.sh *Rakefile" {
 README.rdoc
 Rakefile
 bin=bin{
  rbbt_config
 }
 lib=lib {
  rbbt.rb
  rbbt=rbbt {
   bow=bow{
    bow.rb
    dictionary.rb
    classifier.rb
   }
   sources=sources{
    go.rb
    biocreative.rb
    biomart.rb
    entrez.rb
    organism.rb
    pubmed.rb
    polysearch.rb
   }
   util=util{
    filecache.rb
    open.rb
    tmpfile.rb
    index.rb
    misc.rb
    simpleDSL.rb
    arrayHash.rb
   }
   ner=ner{
    abner.rb
    banner.rb
    regexpNER.rb
    rner.rb
    rnorm.rb
    rnorm=rnorm{
     cue_index.rb
     tokens.rb
    }
   }
  }
 }
 install_scripts=install_scripts{
  get_abner.sh
  get_biocreative.sh
  get_crf++.sh
  get_banner.sh
  get_entrez.sh
  get_go.sh
  get_polysearch.sh
  classifier=classifier{
   R=R{
    classify.R
   }
   Rakefile
  }
  ner=ner{
   Rakefile
   config=config{
    default.rb
    }
  }
  norm=norm{
   functions.sh
   Rakefile
   config=config{
    cue_default.rb
    tokens_default.rb
    }
  }
  organisms=organisms{
   rake-include.rb
   Rakefile
   cgd.Rakefile
   human.Rakefile
   mgi.Rakefile
   pombe.Rakefile
   rgd.Rakefile
   sgd.Rakefile
   tair.Rakefile
   worm.Rakefile
   }
 }
 tasks=tasks {
  deployment.rake
  environment.rake
  website.rake
  install.rake
 }
 test=test {
  test_helper.rb
  test_rbbt.rb
  rbbt=rbbt{
   bow=bow{
    test_bow.rb
    test_classifier.rb
    test_dictionary.rb
   }
   sources=sources{
    test_biocreative.rb
    test_biomart.rb
    test_entrez.rb
    test_go.rb
    test_organism.rb
    test_polysearch.rb
    test_pubmed.rb
   }
   util=util{
    test_arrayHash.rb
    test_filecache.rb
    test_index.rb
    test_misc.rb
    test_open.rb
    test_simpleDSL.rb
    test_tmpfile.rb
   }
   ner=ner{
    test_abner.rb
    test_banner.rb
    test_regexpNER.rb
    test_rner.rb
    test_rnorm.rb
    rnorm=rnorm{
     test_cue_index.rb
     test_tokens.rb
     }
   }
  }
 }
}
